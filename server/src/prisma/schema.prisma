generator defaultGenerator {
  provider = "prisma-client-js"
}

generator prismaClassGenerator {
  provider = "prisma-class-generator"
  output = "../../src/prisma/generated"
  dryRun = false
  useSwagger = false
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model User {
    id Int @id @default(autoincrement())
    email String @unique
    username String
    password String
} 